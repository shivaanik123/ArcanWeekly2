# Production Environment Configuration
# DO NOT commit this file with real values - use AWS Secrets Manager or Parameter Store

# Storage Configuration
USE_S3_STORAGE=true

# S3 Configuration (will be set by CDK deployment)
S3_BUCKET_NAME=arcan-dashboard-data-prod-ACCOUNT_ID
S3_DATA_PREFIX=data/

# AWS Configuration (will be automatically available in EC2)
AWS_REGION=us-east-1

# Application Configuration
STREAMLIT_SERVER_HEADLESS=true
STREAMLIT_SERVER_ENABLE_CORS=false
STREAMLIT_SERVER_ENABLE_XSRF_PROTECTION=false

# Logging
STREAMLIT_LOGGER_LEVEL=WARNING